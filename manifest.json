{
    "name": "CV Prefiller",
    "version": "1.0",
    "description": "A simple CV prefiller extension",
    "manifest_version": 3,
    "permissions": [ 
        "tabs", "activeTab", "storage", "webNavigation"
    ],
    "action": {
        "default_icon": "CV_img.jpeg", 
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": [
            "content_script.js"
          ]
        }
      ]
}
  